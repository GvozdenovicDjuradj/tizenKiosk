{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-var-requires": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "semicolon": [
            true,
            "never",
            "ignore-interfaces"
        ],
        "trailing-comma": [
            true,
            "never"
        ],
        "indent": [
            true,
            "spaces",
            2
        ],
        "ordered-imports": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
